{
    "name": "laravel-notification-channels/lob",
    "description": "Lob.com notifications channel for Laravel 5.3",
    "homepage": "https://github.com/laravel-notification-channels/lob",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "themsaid@gmail.com",
            "homepage": "http://themsaid.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "illuminate/queue": "~5.5 || ~6.0",
        "lob/lob-php": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0 || ~8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Lob\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Lob\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
